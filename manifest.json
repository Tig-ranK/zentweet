{
  "manifest_version": 3,
  "name": "ZenTweet",
  "version": "1.0",
  "description": "Twitter, but Zen. Post without distractions.",
  "host_permissions": [
    "https://twitter.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "css": ["zen.css"],
      "js": ["redirect.js"]
    }
  ]
}
